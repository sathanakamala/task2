#include<iostream>
using namespace std;

int main()
{
    int age;
    cout<<"enter your age:\n";
    cin>>age;
    
    if(age>=18)
    {
        cout<<"You are eligible for voting!!";
    }
    else
       cout<<"Oops!!You are not eligible for voting.";
    return 0;
}





output:
enter your age:
24
You are eligible for voting!!
